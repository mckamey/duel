{
	"isDevMode": false,
	"contentType": "text/html",
	"encoding": "UTF-8",
	"targetDir": "target/www/",
	"sourceDir": "target/foo-web/",
	"serverPrefix": "com.example.web.views",
	"cdnMap": "cdn",
	"cdnLinksMap": "cdnLinks",
	"cdnHost": ".",
	"views": {
		"index.html":
			{
				"view": "HomePage",
				"data": {
					"foo": "bar",
					"blah": "yada"
				},
				"extras": null,
				"appCache": {
					"manifest": "home.appcache",
					"version": "v2",
					"cache": [
						"/favicon.ico"
					],
					"fallback": {
						"/": "/index.html"
					},
					"network": [
						"*"
					]
				}
			},
		"error.html":
			{
				"view": "ErrorPage",
				"data": {
					"foo": "bar",
					"blah": "yada"
				},
				"extras": {
					"App.foo": "bar",
					"App.blah": "yada"
				}
			}
	},
	"files": [
		"robots.txt",
		"favicon.ico"
	]
}